{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true,
    "jest/globals": true
  },
  "extends": [
    "standard"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "quote-props": [
      "error",
      "as-needed",
      {"unnecessary": false}
    ]
  },
  "plugins": ["jest"],
  "globals": {
    "getPackage": "readonly"
  }
}
